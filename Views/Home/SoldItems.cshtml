@model List<Emarket_Website.Models.Order>

<h2>Sold Items</h2>

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            
            <th>Order Date</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
               
                <td>@order.OrderDate</td>
                
            </tr>
        }
    </tbody>
</table>

